{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "graphql",
    "json",
    "svg",
    "typescript",
    "typescriptreact"
  ],
  "languageserver": {
    "sorbet": {
      "command": "bin/srb",
      "args": [
        "tc",
        "--lsp",
        "--enable-all-experimental-lsp-features"
      ],
      "filetypes": [
        "ruby"
      ],
      "rootPatterns": [
        "sorbet/config"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
